<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:ERClipGeneratorTool"
             x:Class="ERClipGeneratorTool.App"
             RequestedThemeVariant="{x:Static ThemeVariant.Dark}">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <SimpleTheme />
        <Style Selector="Window">
            <Setter Property="Icon" Value="/Assets/hbt.ico" />
        </Style>
    </Application.Styles>
</Application>